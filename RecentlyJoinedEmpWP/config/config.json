{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "recently-joined-employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/recentlyJoinedEmployee/RecentlyJoinedEmployeeWebPart.js",
          "manifest": "./src/webparts/recentlyJoinedEmployee/RecentlyJoinedEmployeeWebPart.manifest.json"
        }
      ]
    },
    "office-location-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/officeLocationWp/OfficeLocationWpWebPart.js",
          "manifest": "./src/webparts/officeLocationWp/OfficeLocationWpWebPart.manifest.json"
        }
      ]
    },
    "birthday-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/birthday/BirthdayWebPart.js",
          "manifest": "./src/webparts/birthday/BirthdayWebPart.manifest.json"
        }
      ]
    },
    "anniversary-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/anniversary/AnniversaryWebPart.js",
          "manifest": "./src/webparts/anniversary/AnniversaryWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "RecentlyJoinedEmployeeWebPartStrings": "lib/webparts/recentlyJoinedEmployee/loc/{locale}.js",
    "OfficeLocationWpWebPartStrings": "lib/webparts/officeLocationWp/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "BirthdayWebPartStrings": "lib/webparts/birthday/loc/{locale}.js",
    "AnniversaryWebPartStrings": "lib/webparts/anniversary/loc/{locale}.js"
  }
}
